
<button id="buts"></button>
<div class='container'>
	<div class="row">

		<div class="col-md-12">
			<div class="paged-header">
				<h1>Expenses
				</h1>
			</div>
		</div>
	</div>

<div class="row">

<div class="col-md-12">
<button id="showform" class= 'btn btn-primary' > Add New Expense</button>


<%= render 'form', expense: @expense %>


<div class='container'>
	
			<table id="expenses-table"  class="table table-striped table-bordered" width="100%", data-source = "<%= expenses_path(format: :json)%>">

				<thead>
					<tr>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">id</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">Date</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">Accountname</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">Description</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">created_at</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">udpated_at</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">user_id</th> 
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">category_id</th>
						<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">amount</th>

					</tr>
				</thead>
				<tbody class="table-hover">	
				</tbody>


			</table>
		
</div>
</div>

</div>










