


<div class='container-fluid expense-page'>


	<div class="row">

		<div class="col-md-12">
			<button id="showform" class= 'btn btn-primary expense-btn' > Add New Expense</button>


			<%= render 'form', cashflow: @cashflow %>


			<div class='container'>
				
						<table id="expenses-table"  class="table table-striped table-bordered" width="100%", data-source = "<%= cashflows_expenses_path(format: :json)%>">

							<thead>
								<tr>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">id</th>
									<<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">Tranaction Type</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">Date</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">Accountname</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-4" scope="col">Description</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">created_at</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">udpated_at</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">user_id</th> 
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-1" scope="col">category_id</th>
									<th class="col-xs-12 col-sm-6 col-md-4 col-lg-2" scope="col">amount</th>

								</tr>
							</thead>
							<tbody class="table-hover">	
							</tbody>


						</table>

				</div>
				
		</div>
	</div>

</div>










